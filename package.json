{
  "name": "@jimbojet/types",
  "version": "0.1.0-dev.0",
  "description": "TypeScript utility types for strings, iterables (array, tuple), and interfaces (property, table).",
  "type": "module",
  "private": false,
  "main": "",
  "types": "types/index.d.ts",
  "files": [
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-zhening-luo/types.git"
  },
  "author": "Jimmy Zhening Luo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimmy-zhening-luo/types/issues"
  },
  "homepage": "https://github.com/jimmy-zhening-luo/types#readme",
  "scripts": {
    "eslint": "eslint --fix --cache --cache-strategy content -c eslint.config.js",
    "lint": "npm run lint:config && npm run lint:src && npm run lint:types",
    "lint:config": "npm run lint:config:js",
    "lint:src": "npm run lint:src:ts",
    "lint:types": "npm run lint:types:dts",
    "lint:config:js": "npm run eslint -- *.config.js",
    "lint:src:ts": "npm run eslint -- src/**/*.ts",
    "lint:types:dts": "npm run eslint -- types/**/*.d.ts",
    "prebuild": "npm run lint",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "node dist/test/index.js",
    "prepublishOnly": "npm test",
    "prestart": "npm test",
    "dev": "npm test"
  },
  "devDependencies": {
    "@jimbojet/lint": "~8.0.19",
    "@stylistic/eslint-plugin": "~2.1.0",
    "@typescript-eslint/eslint-plugin": "~7.11.0",
    "@typescript-eslint/parser": "~7.11.0",
    "eslint": "~8.57.0",
    "typescript": "^5.4.5"
